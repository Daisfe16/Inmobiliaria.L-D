@model InmobiliariaMVC.Models.Inquilino

@{
    ViewData["Title"] = "Nuevo Inquilino";
}

<h1>Nuevo Propietario</h1>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="nombre"></label>
        <input asp-for="nombre" class="form-control" />
    </div>
    <div class="form-group"></div>
        <label asp-for="apellido"></label>
        <input asp-for="apellido" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="dni"></label>
        <input asp-for="dni" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="telefono"></label>
        <input asp-for="telefono" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="email"></label>
        <input asp-for="email" class="form-control" />
    </div>

    <button type="submit" class="btn btn-success mt-3">Guardar</button>
    <a asp-action="Index" class="btn btn-secondary mt-3">Cancelar</a>
</form>
